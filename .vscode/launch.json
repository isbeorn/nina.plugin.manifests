    "version": "0.2.0",
    "configurations": [
         {
            "type": "node",
            "request": "launch",
            "name": "Validate All",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\validate-all-manifests.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Validate",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\validate-latest-manifest.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Gather",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\gather.js"
        }
    ]
}